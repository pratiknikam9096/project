{
  "version": 2,
  "builds": [
    {
      "src": "api/feedback.js",
      "use": "@vercel/node"
    },
    {
      "src": "client/package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/.*",
      "dest": "/api/feedback.js"
    },
    {
      "src": "/(.*)",
      "dest": "/client/index.html"
    }
  ]
}
